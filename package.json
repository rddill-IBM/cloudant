{
  "name": "couch-cloudant",
  "version": "1.0.0",
  "private": "public",
  "description": "common no sql access for local couchDB and cloud based Cloudant",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "nyc --reporter=html mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/rddill-IBM/cloudant"
  },
  "keywords": [
    "CouchDB",
    "Cloudant"
  ],
  "author": "Bob Dill, IBM Distinguished Engineer",
  "license": "Apache-2.0",
  "dependencies": {
    "cfenv": "^1.2.2",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "response": "^0.18.0",
    "stream-browserify": "^2.0.2"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0"
  },
  "nyc": {
    "exclude": [
      "test/**",
      "node_modules/**",
      "HTML/**",
      "coverage/**",
      "cloudant/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
