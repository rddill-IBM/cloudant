{
  "name": "@rddill/cloudant",
  "version": "1.0.1",
  "private": false,
  "description": "common no sql access for local couchDB and cloud based Cloudant",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "nyc --reporter=html mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rddill-IBM/cloudant"
  },
  "keywords": [
    "CouchDB",
    "Cloudant"
  ],
  "author": "Bob Dill, IBM Distinguished Engineer",
  "license": "Apache-2.0",
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "request": "^2.88.0"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "exclude": [
      "test/**",
      "node_modules/**",
      "HTML/**",
      "coverage/**",
      ".nyc_output/**",
      "out/**",
      "cloudant/**"
      ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "directories": {
    "test": "test"
  }
}
